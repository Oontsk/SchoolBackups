<commands
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="part2in.xsd"
    spatialWidth="64"
    spatialHeight="64"
    pmOrder="3">
 
 <clearAll/>
 
 <!-- nothing there yet -->
 <listCities sortBy="name"/>
 <listCities sortBy="coordinate"/>
 <printTreap/>
 
 <createCity name="Rome" x="12" y="41" radius="0" color="black"/>
 
 <!-- duplicate name -->
 <createCity name="Rome" x="13" y="42" radius="0" color="black"/>
 
 <!-- duplicate coordinates -->
 <createCity name="Sparta" x="12" y="41" radius="0" color="red"/>
 
 <!-- nothing mapped -->
 <printPMQuadtree/>
 
 <!-- start equals end -->
 <mapRoad start="Rome" end="Rome"/>
 
 <!-- start point does not exist -->
 <mapRoad start="Foo" end="Rome"/>
 
 <!-- end point does not exist -->
 <mapRoad start="Rome" end="Foo"/>
 
 <!-- nameNotInDictionary -->
 <mapCity name="Athens"/>
 
 <mapCity name="Rome"/>
 <createCity name="A" x="40" y="40" radius="0" color="black"/>
 <createCity name="B" x="50" y="50" radius="0" color="black"/>
 <createCity name="Athens" x="500" y="500" radius="0" color="black"/>
 <mapRoad start="A" end="B"/>
 
 <!-- cityAlreadyMapped -->
 <mapCity name="Rome"/>
 <mapCity name="A"/>
 <mapCity name="B"/>
 
 <!-- cityOutOfBounds -->
 <mapCity name="Athens"/>
 
 <clearAll/>
 
 <!-- nothing there anymore -->
 <listCities sortBy="name"/>
 <listCities sortBy="coordinate"/>
 <printTreap/>
</commands>
