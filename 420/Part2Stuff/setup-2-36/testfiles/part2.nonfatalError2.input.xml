<commands
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="part2in.xsd"
    spatialWidth="64"
    spatialHeight="64"
    pmOrder="3">
 
 <createCity name="Rome" x="12" y="41" radius="0" color="black"/>
 <createCity name="Sparta" x="50" y="50" radius="0" color="red"/>
 <createCity name="Athens" x="55" y="55" radius="0" color="black"/>
 <createCity name="Palermo" x="50" y="55" radius="0" color="black"/>
 
 <!-- nothing mapped -->
 <rangeCities x="500" y="500" radius="50"/>
 <rangeRoads x="500" y="500" radius="50"/>
 <nearestCity x="500" y="500"/>
 <nearestRoad x="500" y="500"/>
 <nearestCityToRoad start="Rome" end="Rome"/>
 
 <!-- start point does not exist -->
 <shortestPath start="Foo" end="Rome"/>
 <shortestPath start="Sparta" end="Rome"/>
 
 <mapRoad start="Sparta" end="Rome"/>
 
 <!-- no other cities mapped -->
 <nearestCityToRoad start="Rome" end="Sparta"/>
 
 <!--  cityInRangeOfOtherCity -->
 <createCity name="Rome2" x="14" y="45" radius="100" color="black"/>
 <createCity name="Rome2" x="60" y="60" radius="2" color="black"/>
 <createCity name="Rome3" x="61" y="61" radius="0" color="black"/>
 
 <mapRoad start="Palermo" end="Athens"/>
 
 <!-- end point does not exist -->
 <shortestPath start="Rome" end="Foo"/>
 
 <!-- no path -->
 <shortestPath start="Rome" end="Athens"/>
</commands>
