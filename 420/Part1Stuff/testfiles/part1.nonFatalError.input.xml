<commands
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="part2in.xsd"
	spatialWidth="256"
	spatialHeight="256"
	>

<createCity name="A" x="0" y="0" radius="2" color="red" />

<!-- createCity -->
<!--duplicateCityCoordinates -->
<createCity name="A" x="0" y="0" radius="2" color="red" />

<!-- duplicateCityName -->
<createCity name="A" x="0" y="1" radius="2" color="red" />

<!-- cityInRangeOfOtherCity -->
<createCity name="B" x="0" y="1" radius="1" color="blue" />
<createCity name="B" x="0" y="1" radius="0" color="blue" />
<createCity name="B" x="10" y="10" radius="50" color="blue" />

<!-- deleteCity -->
<!-- cityDoesNotExit -->
<deleteCity name="C" />

<!-- mapCity -->
<!-- nameNotInDictionary -->
<mapCity name="C" />

<!-- cityAlreadyMapped -->
<mapCity name="A" />
<mapCity name="A" />

<!-- cityOutOfBounds -->
<createCity name="D" x="10000" y="10000" radius="0" color="red" />
<mapCity name="D" />

<!-- unmapCity -->
<!-- nameNotInDictionary -->
<unmapCity name="C" />

<!-- cityNotMapped -->
<unmapCity name="D" />

<!-- rangeCities -->
<!-- noCitiesExistInRange -->
<rangeCities x="5" y="5" radius="1" />

<clearAll />

<!-- listCities -->
<!-- noCitiesToList -->
<listCities sortBy="name" />

<!-- printPRQuadtree -->
<!-- mapIsEmpty -->
<printPRQuadtree />

<!-- nearestCity -->
<!-- mapIsEmpty -->
<nearestCity x="5" y="5" />

</commands>
